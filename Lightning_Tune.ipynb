        "# __tune_train_checkpoint_begin\n",
        "def train_StackGAN(config, checkpoint_dir=None, num_epochs=10, num_gpus=torch.cuda.device_count()):\n",
        "    # print(os.cpu_count(), torch.cuda.device_count())\n",
        "    kwargs = {\n",
        "        # 'limit_train_batches' : 0.05,\n",
